{
  "name": "tree-crawl",
  "version": "0.0.0",
  "description": "Generic tree traversal library.",
  "author": "Nicolas Gryman <ngryman@gmail.com> (http://ngryman.sh/)",
  "license": "MIT",
  "repository": "ngryman/tree-crawl",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "eslint {,lib/,test/}*.js",
    "unit": "nyc ava",
    "test": "npm run lint -s && npm run unit -s",
    "dev": "npm run unit -- --watch",
    "coverage": "nyc report --reporter=text-lcov | codecov",
    "see-coverage": "nyc report --reporter=html && open coverage/index.html",
    "check-coverage": "nyc check-coverage --lines 95 --functions 95 --branches 95",
    "docs": "jsdoc2md {,lib/}*.js > docs/api.md && git add docs/api.md"
  },
  "precommit": [
    "test",
    "check-coverage",
    "docs"
  ],
  "keywords": [
    "tree",
    "generic",
    "traverse",
    "traversal",
    "walk",
    "visit",
    "visitor",
    "recursive",
    "depth first",
    "preorder",
    "postorder"
  ],
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.16.0",
    "codecov.io": "^0.1.6",
    "eslint": "^3.1.1",
    "eslint-config-ngryman": "^1.4.0",
    "jsdoc-to-markdown": "^1.3.7",
    "nyc": "^8.0.0",
    "pre-commit": "^1.1.3"
  }
}
